{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "blacklist_service",
        "image": "<tu-imagen-docker>",
        "essential": true,
        "memory": 512,
        "portMappings": [
          {
            "containerPort": 5000,
            "hostPort": 5000
          }
        ],
        "environment": [
          { "name": "DATABASE_URL", "value": "<url-aws-rds>" },
          { "name": "JWT_SECRET_KEY", "value": "<secret>" }
        ]
      }
    ]
}